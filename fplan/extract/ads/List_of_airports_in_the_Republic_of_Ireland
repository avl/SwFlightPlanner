				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub">  (Redirected from <a href="/w/index.php?title=List_of_airports_in_the_Republic_of_Ireland&amp;redirect=no" title="List of airports in the Republic of Ireland">List of airports in the Republic of Ireland</a>)</div>
